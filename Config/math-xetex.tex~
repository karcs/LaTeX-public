\newcommand{\leftsemidirprod}[1][]{\mathbin{\ifx&#1&\ltimes\else{\ltimes_{#1}}\fi}}
\newcommand{\rightsemidirprod}[1][]{\ifx#1\empty\rtimes\else{\rtimes_{#1}}\fi}
